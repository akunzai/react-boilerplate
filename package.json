{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env REACT_APP_MOCK=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint src/**/*.{ts,tsx}",
    "i18n:extract": "i18next"
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.2.1",
    "@types/jest": "27.0.2",
    "@types/node": "16.10.2",
    "@types/react": "17.0.27",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.3.0",
    "bootstrap": "5.1.1",
    "bootstrap-icons": "1.5.0",
    "formik": "2.2.9",
    "i18next": "21.2.4",
    "i18next-browser-languagedetector": "6.1.2",
    "react": "17.0.2",
    "react-cool-onclickoutside": "1.6.2",
    "react-dom": "17.0.2",
    "react-i18next": "11.12.0",
    "react-router-dom": "5.3.0",
    "react-scripts": "4.0.3",
    "react-service-container": "0.2.6",
    "rxjs": "7.3.1",
    "sass": "1.39.2",
    "typescript": "4.3.5",
    "web-vitals": "2.1.0"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "i18next-parser": "5.0.0",
    "msw": "0.35.0",
    "react-test-renderer": "17.0.2"
  },
  "resolutions": {
    "browserslist": "4.17.2",
    "glob-parent": "6.0.2",
    "immer": "9.0.6",
    "set-value": "^4.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/i18n.ts",
      "!src/i18nForTests.ts",
      "!src/mocks/**",
      "!src/reportWebVitals.ts",
      "!src/setupTests.ts"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "packageManager": "yarn@3.0.2"
}
