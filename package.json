{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "private": true,
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "start": "cross-env REACT_APP_MOCK=true GENERATE_SOURCEMAP=false react-scripts start",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint src/**/*.{ts,tsx}",
    "i18n:extract": "i18next"
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "28.1.8",
    "@types/node": "18.7.9",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-router-dom": "5.3.3",
    "bootstrap": "5.2.0",
    "bootstrap-icons": "1.9.1",
    "formik": "2.2.9",
    "i18next": "21.9.1",
    "i18next-browser-languagedetector": "6.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "11.18.5",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "react-service-container": "0.2.6",
    "rooks": "6.4.3",
    "rxjs": "7.5.6",
    "sass": "1.54.5",
    "typescript": "4.8.2",
    "web-vitals": "3.0.0"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "graphql": "16.6.0",
    "i18next-parser": "6.5.0",
    "msw": "0.45.0",
    "react-test-renderer": "18.2.0"
  },
  "resolutions": {
    "async": "3.2.4",
    "browserslist": "4.21.3",
    "ejs": "3.1.8",
    "follow-redirects": "1.15.1",
    "glob-parent": "6.0.2",
    "immer": "9.0.15",
    "minimist": "1.2.6",
    "nanoid": "3.3.4",
    "nth-check": "2.1.1",
    "node-forge": "1.3.1",
    "terser": "5.15.0",
    "set-value": "4.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.ts",
      "!src/index.tsx",
      "!src/i18n.ts",
      "!src/i18nForTests.ts",
      "!src/mocks/**",
      "!src/reportWebVitals.ts",
      "!src/setupTests.ts",
      "!src/types.ts"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
